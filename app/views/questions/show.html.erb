<h4>Quiz: <%= @question.quiz.name %></h4>

<h4>Question: <%= @question.body %> </h4>
<%= link_to 'Edit', edit_question_path(@question.id) %>

<div>
  <% @answers.each do |answer| %>
      <div>
        <p><%= answer.body %></p>

        <small>
          <%= link_to 'Edit', edit_answer_path(answer.id) %>
        </small> 

      </div>
  <% end %>
</div>


